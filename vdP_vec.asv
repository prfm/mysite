function z = inputmaker(y,A,klong,index)
% Return the index-th input of the function 
    n = size(y,1);
    s = size(A,1);
    z = zeros(n,1);

    
    for i=1:s
        z = z + A(index,i)*klong(n*(i-1)+1:n*i);
    end
    
    z = z + y;
end

